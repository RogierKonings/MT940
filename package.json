{
  "name": "mt940",
  "version": "0.0.0",
  "description": "MT940 validator",
  "scripts": {
    "ng": "ng",
    "start": "run-p dev start-mockserver",
    "start-prod": "ng serve --prod --aot",
    "start-mockserver": "ts-node ./mock-server/server.js",
    "start-mockserver-standalone": "ts-node ./mock-server/server.js --standalone=true",
    "dev": "ng serve --proxy-config ./mock-server/proxy.config.json -o",
    "build": "ng build",
    "build-prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod --aot",
    "test": "ng test --watch=false",
    "e2e": "npm run cy:open",
    "release": "npm-run-all -s clean lint test it:cy build-prod",
    "lint": "ng lint --type-check",
    "clean": "rimraf dist",
    "it:cy": "npm-run-all -p -r start-mockserver-standalone cy:run",
    "cy:open": "run-p start-mockserver-standalone cypress:open",
    "cy:run": "cypress run --browser chrome",
    "cypress:open": "cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.2.14",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "bootstrap": "4.4.1",
    "iban": "0.0.12",
    "rxjs": "6.4.0",
    "stream-browserify": "2.0.2",
    "timers-browserify": "2.0.11",
    "tslib": "1.10.0",
    "xml2js": "0.4.23",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.803.22",
    "@angular/cli": "8.3.22",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@cypress/webpack-preprocessor": "4.1.1",
    "@types/cucumber": "6.0.0",
    "@types/express": "4.17.2",
    "@types/iban": "0.0.30",
    "@types/jasmine": "3.3.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "8.9.4",
    "@types/xml2js": "0.4.5",
    "codelyzer": "5.0.0",
    "cors": "2.8.5",
    "cucumber": "6.0.5",
    "cypress": "3.8.2",
    "cypress-cucumber-preprocessor": "2.0.1",
    "cypress-file-upload": "3.5.3",
    "express": "4.17.1",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "ng-apimock": "1.4.9",
    "npm-run-all": "4.1.5",
    "path": "0.12.7",
    "rimraf": "3.0.0",
    "ts-node": "7.0.0",
    "tslint": "5.15.0",
    "typescript": "3.5.3"
  }
}
