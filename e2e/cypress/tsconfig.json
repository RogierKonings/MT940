{
  "compilerOptions": {
    "baseUrl": "..",
    "paths": {
      "*": ["types/*"]
    },
    "outDir": "./dist",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "types": ["cypress", "node"]
  },
  "include": [
    "../node_modules/cypress",
    "*/*.ts"
  ]
}
